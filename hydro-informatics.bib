@article{hassan2018spgm,
  title = {{{SPGM}}: {{A Scalable PaleoGeomorphology Model}}},
  author = {Hassan, Rakib and Gurnis, Michael and Williams, Simon E. and M{\"u}ller, R. Dietmar},
  year = {2018},
  journal = {SoftwareX},
  volume = {7},
  pages = {263--272},
  issn = {2352-7110},
  doi = {10.1016/j.softx.2018.07.005},
  abstract = {Numerical models of landscape evolution are playing an increasingly important role in providing an improved understanding of geomorphic transport processes shaping Earth{\dbend}s surface topography. Improving theoretical underpinnings coupled with increasing computational capacity has led to the development of several open source codes written in low-level languages. However, adapting these codes to new functionality or introducing greater flexibility often requires significant recoding. Here we present a multi-process, scalable, numerical model of geomorphological evolution, built with a modular structure and geared toward seamless extensibility. We implement recent algorithmic advances that reduce the computational cost of flow routing {\dbend}{\dbend}a problem that typically scales quadratically with the number of unknowns {\dbend}{\dbend}to linear in time while allowing for parallel implementations of geomorphic transport processes. Our scalability tests demonstrate that such parallelizations can achieve an order of magnitude speedup on a typical desktop computer, making large-scale simulations more tractable.},
  keywords = {Geomorphology,Landscape evolution model,Morphodynamics,Paleogeomorphology,Surface processes}
