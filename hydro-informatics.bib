  title = {Deoxygenation Alters Bacterial Diversity and Community Composition in the Ocean's Largest Oxygen Minimum Zone},
  author = {Beman, J. Michael and Carolan, Molly T.},
  year = {2013},
  month = oct,
  journal = {Nature Communications},
  volume = {4},
  number = {1},
  pages = {2705},
  publisher = {{Nature Publishing Group}},
