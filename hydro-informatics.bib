@article{madsen2000automatic,
  title = {Automatic Calibration of a Conceptual Rainfall{\textendash}Runoff Model Using Multiple Objectives},
  author = {Madsen, H},
  year = {2000},
  month = aug,
  journal = {Journal of Hydrology},
  volume = {235},
  number = {3-4},
  pages = {276--288},
  issn = {00221694},
  doi = {10.1016/S0022-1694(00)00279-1},
  urldate = {2020-05-26},
  abstract = {Formulation of an automatic calibration strategy for the MIKE 11/NAM rainfall{\textendash}runoff model is outlined. The calibration scheme includes optimisation of multiple objectives that measure different aspects of the hydrograph: (1) overall water balance, (2) overall shape of the hydrograph, (3) peak flows, and (4) low flows. An automatic optimisation procedure based on the shuffled complex evolution algorithm is introduced for solving the multi-objective calibration problem. A test example is presented that illustrates the principles and implications of using multiple objectives in model calibration. Significant trade-offs between the different objectives are observed in this case and no single unique set of parameter values is able to optimise all objectives simultaneously. Instead, the solution to the calibration problem is given as a set of Pareto optimal solutions, which from a multi-objective viewpoint are equivalent. A large variability is observed in the Pareto optimal parameter sets, resulting in a large range of ``equally good'' simulated hydrographs. From the set of Pareto optimal solutions, one can draw a single solution according to priorities of the different objectives for the specific model application being considered. A balanced aggregated objective function is proposed, which provides a compromise solution that puts equal weights to the different objectives. ᭧ 2000 Elsevier Science B.V. All rights reserved.},
  langid = {english}
}

@inproceedings{madsen2000automatica,
  title = {Automatic {{Calibration}} and {{Uncertainty Assessment}} in {{Rainfall-Runoff Modelling}}},
  booktitle = {Building {{Partnerships}}},
  author = {Madsen, Henrik},
  year = {2000},
  month = sep,
  pages = {1--10},
  publisher = {{American Society of Civil Engineers}},
  address = {{Minneapolis, Minnesota, United States}},
  doi = {10.1061/40517(2000)147},
  urldate = {2020-05-26},
  abstract = {An automatic calibration procedure for the MIKE 11/NAM rainfall-runoff model is described. The procedure considers multiple calibration objectives, including (1) a good simulation of the water balance, (2) a good overall agreement of the shape of the hydrograph, (3) a good agreement of peak flows, and (4) a good agreement for low flows. The shuffled complex evolution algorithm is applied for optimising the different calibration objectives simultaneously. The solution to the multi-objective calibration problem is given as a set of Pareto optimal solutions. From this set one can draw a single solution according to priorities of the different calibration objectives for the specific model application being considered. The calibration algorithm can explicitly take data and model errors into account by defining appropriate weights that indicate the importance to be given to particular portions of the hydrograph in the calibration process. Furthermore, a methodology for assessing parameter uncertainty is outlined. The procedure is based on a comparison of the distributions of the model parameters corresponding to the best and worst model simulations, respectively, obtained from a Monte Carlo sampling. An application example is presented that illustrates the use of the developed procedures.},
  isbn = {978-0-7844-0517-8},
  langid = {english}
}

@article{madsen2001automatic,
  title = {Automatic Calibration of an Integrated Hydrological Modelling System},
  author = {Madsen, Henrik},
  year = {2001},
  pages = {20},
  abstract = {In this paper, automatic calibration of an integrated hydrological modelling system, MIKE SHE, is considered. A consistent framework for automatic calibration is formulated which focuses on the different steps in the estimation process from model parameterisation and selection of calibration parameters, formulation of calibration criteria, and choice of optimisation algorithm. The calibration problem is formulated in a general multi-objective context in which different objective functions that measure individual process descriptions can be optimised simultaneously. A test example is presented that illustrates the use of the automatic calibration scheme. Different sensitivity tests are performed to demonstrate the trade-off between the calibration objectives with respect to the use of different calibration data and objective function measures.},
  langid = {english}
}

@article{madsen2002comparison,
  title = {Comparison of Different Automated Strategies for Calibration of Rainfall-Runoff Models},
  author = {Madsen, Henrik and Wilson, Geoffrey and Ammentorp, Hans Christian},
  year = {2002},
  month = apr,
  journal = {Journal of Hydrology},
  volume = {261},
  number = {1-4},
  pages = {48--59},
  issn = {00221694},
  doi = {10.1016/S0022-1694(01)00619-9},
  urldate = {2020-05-26},
  abstract = {Three different automated methods for calibration of rainfall-runoff models are presented and compared. The methods represent various calibration strategies that utilise multiple objectives and allow user intervention on different levels and different stages in the calibration process. The methods have been applied for calibration of a test catchment and compared on validation data with respect to overall performance measures in terms of water balance error and general hydrograph shape, and simulation of high and low {\textasciimacron}ow events. The results illustrate the problem of non-uniqueness in model calibration since none of the methods are superior with respect to all performance measures considered. In general, the different methods put emphasis on different response modes of the hydrograph. Calibration based on the use of generic search routines in combination with user-speci{\textregistered}ed calibration priorities is seen to compare favourably with an expert system that is designed for the speci{\textregistered}c model being considered and requires user intervention during the entire calibration process. q 2002 Elsevier Science B.V. All rights reserved.},
  langid = {english}
}

@article{madsen2003parameter,
  title = {Parameter Estimation in Distributed Hydrological Catchment Modelling Using Automatic Calibration with Multiple Objectives},
  author = {Madsen, Henrik},
  year = {2003},
  month = feb,
  journal = {Advances in Water Resources},
  volume = {26},
  number = {2},
  pages = {205--216},
  issn = {03091708},
  doi = {10.1016/S0309-1708(02)00092-1},
  urldate = {2020-05-26},
  abstract = {A consistent framework for parameter estimation in distributed hydrological catchment modelling using automatic calibration is formulated. The framework focuses on the different steps in the estimation process from model parameterisation and selection of calibration parameters, formulation of calibration criteria, and choice of optimisation algorithm. The calibration problem is formulated in a general multi-objective context in which different objective functions that measure individual process descriptions can be optimised simultaneously. Within this framework it is possible to tailor the model calibration to the specific objectives of the model application being considered. A test example is presented that illustrates the use of the calibration framework for parameter estimation in the MIKE SHE integrated and distributed hydrological modelling system. A significant trade-off between the performance of the groundwater level simulations and the catchment runoff is observed in this case, defining a Pareto front with a very sharp structure. The Pareto optimum solution corresponding to a proposed balanced aggregated objective function is seen to provide a proper balance between the two objectives. Compared to a manual expert calibration, the balanced Pareto optimum solution provides generally better simulation of the runoff, whereas virtually similar performance is obtained for the groundwater level simulations.},
  langid = {english}
}

