@incollection{nakagawa1993turbulencemodel,
  title = {Turbulence-Model Calculations of Open-Channel Flows},
  booktitle = {Turbulence in {{Open Channel Flows}}},
  author = {Nakagawa, Hiroji, Iehisa Nezu},
  year = {1993},
  pages = {25},
  publisher = {{Routledge}},
  abstract = {Turbulence in open-channel flows consists of motions with a wide spectrum of scales ranging from millimeters to the flow depth, see Figure 7.1. The motions at all scales are governed by the exact time-dependent Navier-Stokes and continuity equations, and numerical methods are now available for solving these equations. However, in spite of the great advances in computer technology, such direct simulation resolving the motions of all scales is possible only for strongly idealized situations at low Reynolds numbers, for which the spectrum of scales is not so wide. At the high Reynolds numbers usually encountered in practice, the size of the smallest turbulence eddies is only some 10{\textendash}4 to 10{\textendash}3 of the water depth. With presently available supercomputers, direct simulations of closed-channel flow for a Reynolds number of 3300 have been conducted, but they required 4 million grid points and 250 hours of supercomputer time (Kim et al. 1987). The results obtained are significant because they reveal all of the structural behavior of turbulence that has been observed experimentally.},
  isbn = {978-0-203-73490-2}
}

@article{nakajima2019new,
  title = {A New Small Device Made of Glass for Separating Microplastics from Marine and Freshwater Sediments},
  author = {Nakajima, Ryota and Tsuchiya, Masashi and Lindsay, Dhugal J. and Kitahashi, Tomo and Fujikura, Katsunori and Fukushima, Tomohiko},
  year = {2019},
  month = oct,
  journal = {PeerJ},
  volume = {7},
  pages = {e7915},
  issn = {2167-8359},
  doi = {10.7717/peerj.7915},
  urldate = {2021-05-14},
  abstract = {Separating microplastics from marine and freshwater sediments is challenging, but necessary to determine their distribution, mass, and ecological impacts in benthic environments. Density separation is commonly used to extract microplastics from sediments by using heavy salt solutions, such as zinc chloride and sodium iodide. However, current devices/apparatus used for density separation, including glass beakers, funnels, upside-down funnel-shaped separators with a shut-off valve, etc., possess various shortcomings in terms of recovery rate, time consumption, and/or usability. In evaluating existing microplastic extraction methods using density separation, we identified the need for a device that allows rapid, simple, and efficient extraction of microplastics from a range of sediment types. We have developed a small glass separator, without a valve, taking a hint from an Uterm{\"o}hl chamber. This new device is easy to clean and portable, yet enables rapid separation of microplastics from sediments. With this simple device, we recovered 94{\textendash}98\% of {$<$}1,000 {\textmu}m microplastics (polyethylene, polypropylene, polyvinyl chloride, polyethylene terephthalate, and polystyrene). Overall, the device is efficient for various sizes, polymer types, and sediment types. Also, microplastics collected with this glass-made device remain chemically uncontaminated, and can, therefore, be used for further analysis of adsorbing contaminants and additives on/to microplastics.},
  langid = {english}
}

