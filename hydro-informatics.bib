@inproceedings{felix2021reduction,
  title = {Reduction of Reservoir Sedimentation by Increasing the Fine Sediment Transport through the Power Waterways of Hydropower Plants},
  booktitle = {{{IAHR}} 2020 - {{Abstract Book}}},
  author = {Felix, David and Albayrak, Ismail and Boes, Robert M},
  editor = {Kalinowska, Monika and Rowi{\'n}ski, Pawe{\l} and Okruszko, Tomasz and Nones, Michael},
  year = {2021},
  month = feb,
  series = {Reservoir Sedimentation: Processes and Management Strategies},
  pages = {2},
  address = {{Warsaw, Poland}},
  abstract = {There are many options to reduce reservoir sedimentation. For hydropower reservoirs, one option is to increase the transport of fine sediments through power waterways and hence the turbines. This has ecological advantages because the suspended sediment concentrations (SSC) in downstream river reaches are similar to pre-dam conditions. However, the operation costs increase due to (i) additional efforts for sediment handling (measures to reduce settling and/or re-suspend sediment) and (ii) hydro-abrasive erosion on turbines. It is proposed to determine the acceptable SSC by balancing these costs against the value of the maintained or restored reservoir volume. Although technical solutions for sediment handling and turbine erosion mitigation are available, further research including comprehensive field tests is recommended.},
  isbn = {978-83-66847-01-9},
  langid = {english}
}

@article{feng2020twodimensional,
  title = {Two-Dimensional Numerical Simulation of Sediment Transport Using Improved Critical Shear Stress Methods},
  author = {Feng, Zhiyong and Tan, Guangming and Xia, Junqiang and Shu, Caiwen and Chen, Peng and Yi, Ran},
  year = {2020},
  month = feb,
  journal = {International Journal of Sediment Research},
  volume = {35},
  number = {1},
  pages = {15--26},
  issn = {10016279},
  doi = {10.1016/j.ijsrc.2019.10.003},
  urldate = {2020-08-10},
  abstract = {Research on the critical shear stresses for erosion and deposition for cohesive sediment has attracted substantial attention from both engineering and theoretical viewpoints due to their importance in sediment transport theory. Previous studies have proposed a large number of empirical and semiempirical methods to estimate the critical erosion and deposition shear stress, but comparative analyses and validation of the existing methods are still lacking, leaving questions regarding the applicability ranges of the methods. The current paper evaluates the performance and applicability range of five critical erosion shear stress methods derived from different hypotheses on sediment transport for flume experiments and natural tidal rivers using a process-based model. In addition, the effect of the critical deposition shear stress on sediment transport is investigated. The results show that the different critical erosion shear stress methods yield distinctly different prediction results, and their performance and applicability ranges are discussed by comparing their predictions with measured sediment concentrations from the Shenzhen River and measured geometric changes from the Partheniades' flume experiment. The hiding and exposure effect has been recognized as a crucial factor in the incipient motion of sediment on nonuniform beds. A sensitivity analysis of selective deposition and continuous deposition justifies the existence of the critical deposition shear stress. The current study highlights the performance and applicability ranges of the existing critical shear stress methods in sediment transport modeling for uniform and nonuniform beds, which will enrich understanding of the underlying mechanisms of erosion and deposition of cohesive sediment.},
  langid = {english}
}

