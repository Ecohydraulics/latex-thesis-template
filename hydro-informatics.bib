@article{tonina2007hyporheic,
  title = {Hyporheic Exchange in Gravel Bed Rivers with Pool-Riffle Morphology: {{Laboratory}} Experiments and Three-Dimensional Modeling},
  shorttitle = {Hyporheic Exchange in Gravel Bed Rivers with Pool-Riffle Morphology},
  author = {Tonina, Daniele and Buffington, John M.},
  year = {2007},
  journal = {Water Resources Research},
  volume = {43},
  number = {1},
  issn = {1944-7973},
  doi = {10.1029/2005WR004328},
  urldate = {2021-09-14},
  abstract = {We report the first laboratory simulations of hyporheic exchange in gravel pool-riffle channels, which are characterized by coarse sediment, steep slopes, and three-dimensional bed forms that strongly influence surface flow. These channels are particularly important habitat for salmonids, many of which are currently at risk worldwide and which incubate their offspring within the hyporheic zone. Here we perform a set of laboratory experiments examining the effects of discharge and bed form amplitude on hyporheic exchange, with surface-subsurface mixing measured directly from the concentration decay of a conservative tracer (fluorescein) injected into the surface flow. Near-bed pressure measurements were also used to predict hyporheic exchange from a three-dimensional pumping transport model. Comparison of the predicted and observed hyporheic exchange shows good agreement, indicating that the major mechanism for exchange is bed form{\textendash}induced advection. However, the effect of bed forms is modulated by discharge and the degree of topographic submergence. We also tested the performance of the hydrostatic pressure as a proxy for the observed near-bed pressure in driving hyporheic exchange, which would facilitate field measurement and analysis of hyporheic flow in natural rivers. We found agreement with measured hyporheic exchange only for low bed form amplitudes and high flows.},
  langid = {english},
  keywords = {gravel bed rivers,hyporheic exchange,pool-riffle morphology}
}

@article{tonina2009threedimensional,
  title = {A Three-Dimensional Model for Analyzing the Effects of Salmon Redds on Hyporheic Exchange and Egg Pocket Habitat},
  author = {Tonina, Daniele and Buffington, John M.},
  editor = {Bradford, Michael},
  year = {2009},
  month = dec,
  journal = {Canadian Journal of Fisheries and Aquatic Sciences},
  volume = {66},
  number = {12},
  pages = {2157--2173},
  issn = {0706-652X, 1205-7533},
  doi = {10.1139/F09-146},
  urldate = {2019-07-15},
  abstract = {A three-dimensional fluid dynamics model is developed to capture the spatial complexity of the effects of salmon redds on channel hydraulics, hyporheic exchange, and egg pocket habitat. We use the model to partition the relative influences of redd topography versus altered hydraulic conductivity (winnowing of fines during spawning) on egg pocket conditions for a simulated pool{\textendash}riffle channel with a redd placed at the pool tail. Predictions show that altered hydraulic conductivity is the primary factor for enhancing hyporheic velocities and dissolved oxygen content within the egg pocket. Furthermore, the simulations indicate that redds induce hyporheic circulation that is nested within that caused by pool{\textendash}riffle topography and that spawning-related changes in hyporheic velocities and dissolved oxygen content could create conditions suitable for incubation in locations that otherwise would be unfavorable (reinforcing the notion that salmonids actively modify their environment in ways that may be beneficial to their progeny). Re{\textasciiacute}sume{\textasciiacute} : Nous mettons au point un mode`le tridimensionnel de dynamique des fluides afin de capturer la complexite{\textasciiacute} spatiale des effets des fraye`res a` saumons sur l'hydraulique du chenal, les e{\textasciiacute}changes hyporhe{\textasciiacute}iques et l'habitat des poches d'{\oe}ufs. Le mode`le nous sert a` de{\textasciiacute}partager l'influence relative de la topographie des fraye`res par rapport a` celle de la conductivite{\textasciiacute} hydraulique alte{\textasciiacute}re{\textasciiacute}e (la se{\textasciiacute}paration des particules fines durant la fraie) sur les conditions des poches d'{\oe}ufs dans un canal expe{\textasciiacute}rimental simulant une alternance de zones profonde et rapide et posse{\textasciiacute}dant une fraye`re place{\textasciiacute}e a` l'aval de la zone profonde. Les pre{\textasciiacute}dictions montrent que le changement de conductivite{\textasciiacute} hydraulique est le facteur principal qui augmente les vitesses hyporhe{\textasciiacute}iques et le contenu en oxyge`ne dissous au sein de la poche des {\oe}ufs. De plus, les simulations indiquent que les fraye`res produisent une circulation hyporhe{\textasciiacute}ique qui est embo{\textasciicircum}{\i}te{\textasciiacute}e dans celle cause{\textasciiacute}e par la topographie des zones profonde et rapide; les changements relie{\textasciiacute}s a` la fraie dans la vitesse hyporhe{\textasciiacute}ique et le contenu en oxyge`ne dissous pourraient cre{\textasciiacute}er des conditions ade{\textasciiacute}quates pour l'incubation dans des sites qui seraient par ailleurs inade{\textasciiacute}quats (ce qui renforce la notion que les salmonide{\textasciiacute}s modifient activement leur environnement selon des modes qui peuvent e{\textasciicircum}tre be{\textasciiacute}ne{\textasciiacute}fiques a` leurs rejetons).},
  langid = {english}
}

@incollection{tonina2013hydraulic,
  title = {Hydraulic {{Modelling Approaches}} for {{Ecohydraulic Studies}}: {{3D}}, {{2D}}, {{1D}} and {{Non-Numerical Models}}},
