@article{srivastava2017plant,
  title = {Plant{\textendash}Microbe Interaction in Aquatic System and Their Role in the Management of Water Quality: A Review},
  shorttitle = {Plant{\textendash}Microbe Interaction in Aquatic System and Their Role in the Management of Water Quality},
  author = {Srivastava, Jatin K. and Chandra, Harish and Kalra, Swinder J. S. and Mishra, Pratibha and Khan, Hena and Yadav, Poonam},
  year = {2017},
  month = jun,
  journal = {Applied Water Science},
  volume = {7},
  number = {3},
