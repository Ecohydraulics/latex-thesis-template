@article{shakibainia2010threedimensional,
  title = {Three-Dimensional Numerical Study of Flow Structure in Channel Confluences},
  author = {Shakibainia, Ahmad and Tabatabai, Mohammad Reza Majdzadeh and Zarrati, Amir Reza},
  year = {2010},
  month = may,
  journal = {Canadian Journal of Civil Engineering},
  volume = {37},
  number = {5},
  pages = {772--781},
  issn = {0315-1468, 1208-6029},
  doi = {10.1139/L10-016},
  urldate = {2020-05-26},
  abstract = {Channel confluence is one of the important features of each river system and some hydraulic structures. The features that can dominantly control flow characteristics in a confluence are confluence angle, discharge, width ratios, and Froude number of flow. Several research studies have been conducted however a comprehensive three-dimensional (3-D) numerical study of flow characteristics in a confluence has not yet been reported. In the present study, SSIIM2.0, a 3-D numerical model, is validated and applied to investigate secondary currents, velocity distribution, flow separation, and water surface elevation in different conditions. The results of the present study illustrate that flow structure and water surface variations in a confluence are highly influenced by confluence angle, discharge, and width ratios as well as Froude number because of their effect on flow deflection, separation, and secondary currents. The graphs from the present study can be used to analyze water surface variation, velocity field, and flow separation dimensions in difference conditions for engineering designs. Key words: channel confluence, flow structure, numerical model, secondary currents, three-dimensional model. Re{\textasciiacute}sume{\textasciiacute} : La confluence des canaux est l'une des caracte{\textasciiacute}ristiques importantes de chaque re{\textasciiacute}seau hydrographique et de certains ouvrages hydrauliques. Les attributs qui peuvent principalement affecter les caracte{\textasciiacute}ristiques de l'e{\textasciiacute}coulement dans une confluence sont l'angle de confluence, les rapports de{\textasciiacute}bit et largeur, de me{\textasciicircum}me que le nombre de Froude de l'e{\textasciiacute}coulement. Plusieurs travaux de recherche ont e{\textasciiacute}te{\textasciiacute} re{\textasciiacute}alise{\textasciiacute}s, mais une e{\textasciiacute}tude nume{\textasciiacute}rique tridimensionnelle comple`te des caracte{\textasciiacute}ristiques de l'e{\textasciiacute}coulement dans une confluence n'a pas encore e{\textasciiacute}te{\textasciiacute} signale{\textasciiacute}e. Dans la pre{\textasciiacute}sente e{\textasciiacute}tude, le mode`le nume{\textasciiacute}rique tridimensionnel SSIIM2,0 est valide{\textasciiacute} et utilise{\textasciiacute} pour examiner des courants secondaires, la distribution de la vitesse, la se{\textasciiacute}paration de l'e{\textasciiacute}coulement et l'e{\textasciiacute}le{\textasciiacute}vation de la surface de l'eau sous diverses conditions. Les re{\textasciiacute}sultats montrent que les variations dans la structure d'e{\textasciiacute}coulement et la surface de l'eau dans une confluence sont tre`s influence{\textasciiacute}es par l'angle de confluence, les rapports de{\textasciiacute}bit et largeur de me{\textasciicircum}me que par le nombre de Froude en raison de leur effet sur la de{\textasciiacute}viation de l'e{\textasciiacute}coulement, la se{\textasciiacute}paration des courants et les courants secondaires. Les graphiques qui en de{\textasciiacute}coulent peuvent e{\textasciicircum}tre utilise{\textasciiacute}s pour analyser la variation de la surface de l'eau, le champ de vitesse et les volumes lors de la se{\textasciiacute}paration de l'e{\textasciiacute}coulement sous diverses conditions pour aider a` la conception technique. Mots-cle{\textasciiacute}s : confluence de canaux, structure d'e{\textasciiacute}coulement, mode`le nume{\textasciiacute}rique, courants secondaires, mode`le tridimensionnel.},
  langid = {english}
}

@article{shams2002computational,
  title = {Computational Modeling of Flow and Sediment Transport and Deposition in Meandering Rivers},
  author = {Shams, Mehrzad and Ahmadi, Goodarz and Smith, Duane H},
  year = {2002},
  month = jun,
  journal = {Advances in Water Resources},
  volume = {25},
  number = {6},
  pages = {689--699},
  issn = {03091708},
  doi = {10.1016/S0309-1708(02)00034-9},
  urldate = {2020-05-26},
  abstract = {A computational modeling analysis of the flow and sediment transport, and deposition in meandering-river models was performed. The Reynolds stress transport model of the FLUENTTM code was used for evaluating the river flow characteristics, including the mean velocity field and the Reynolds stress components. The simulation results were compared with the available experimental data of the river model and discussed. The Lagrangian tracking of individual particles was performed, and the transport and deposition of particles of various sizes in the meandering river were analyzed. Particular attention was given to the sedimentation patterns of different size particles in the river-bend model. The flow patterns in a physical river were also studied. A Froude number based scale ratio of 1:100 was used, and the flow patterns in the physical and river models are compared. The result shows that the mean-flow quantities exhibit dynamic similarity, but the turbulence parameters of the physical river are different from the model. More strikingly, the particle sedimentation features in the physical and river models do not obey the expected similarity scaling.},
  langid = {english}
}

