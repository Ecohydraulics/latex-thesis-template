  author = {Sankalp, Sovan and Khatua, {\relax Kishanjit}. K. and Pradhan, Arpan},
  year = {2015},
  journal = {Aquatic Procedia},
  volume = {4},
  pages = {812--818},
  issn = {2214241X},
  doi = {10.1016/j.aqpro.2015.02.101},
  urldate = {2020-05-26},
  abstract = {Reliable assessment of boundary shear distribution in open channel is of vital importance in analyzing various critical engineering problems such as channel design, channel migration, energy loses, sedimentation etc. An investigation concerning the distribution of shear stress in meandering channels has been represented .An analysis is also made to study the relationships of boundary shear distribution for different geometrical and hydraulic parameters such as sinuosity, aspect ratio, Reynolds number, Froude's number. Bend apex being the section of maximum curvature in a meandering channel, the longitudinal velocity remains higher on the inner wall as compared to the outer wall in a meandering channel. Hence the shear stress also remains higher on the inner wall. Comparing the work of other investigators having different channel parameters gives an overall aspect of shear stress distribution. Therefore these features can be considered by engineers and researches in the field of bank erosion and deposition, navigation, bank protection, etc.},
  langid = {english}
}

