@inproceedings{gold2021effect,
  title = {The Effect of Reservoir Flushing on Existing Headwater Delta Formations of Run-of- River Hydropower Plants {\textendash} {{Experimental}} Studies with Nonuniform Sediments},
  booktitle = {{{IAHR}} 2020 - {{Abstract Book}}},
  author = {Gold, Thomas and Reiterer, Kevin and Hauer, Christoph and Habersack, Helmut and Sindelar, Christine and Kalinowska, Monika and Rowi{\'n}ski, Pawe{\l} and Okruszko, Tomasz and Nones, Michael},
