@incollection{meshkova2012nomenclature,
  title = {Nomenclature, {{Complexity}}, {{Semi-Alluvial Channels}} and {{Sediment-Flux-Driven Bedrock Erosion}}},
  booktitle = {Gravel-{{Bed Rivers}}},
  author = {Meshkova, Lyubov V. and Carling, Paul A. and {Buffin-B{\'e}langer}, Thomas},
  year = {2012},
  pages = {424--431},
  publisher = {{John Wiley \& Sons, Ltd}},
  doi = {10.1002/9781119952497.ch31},
  urldate = {2022-12-15},
  abstract = {Recent research concerning the evolution of rock-bed channels largely has focussed on the incision processes, often related to bulk streampower, expressed as a function of basin-scale catchment area and reach-scale channel slope. In contrast, Turowksi (this volume) draws attention to the local `tools' versus `cover' effect of often partial sediment cover within rock-bed channels and the effects on channel morphology. Herein we introduce some additional, neglected concepts with new, proposed terminology. Importantly, we emphasize the probable role of the sediment cover in maintaining dynamic equilibria in the rock-bed channel incision process. Although `bedrock-confined' channels often contain a basal sediment filament, some channels have a rock bed but alluvial banks {\textendash} here termed `bedrock-constrained' channels. In plan, some bedrock-constrained rivers exhibit alluvial, meandering behaviour superimposed as an `alluvial overprint' whereas local hard points in semi-alluvial rivers modify alluvial patterns as the `bedrock footprint'. Local hard points may also occur as stabilizing vegetation or woody debris which act in a manner similar to some engineered structures. Thus understanding of semi-alluvial channels might be enhanced by comparison with the well-researched role of the effects of engineered structures on alluvial channels.},
  chapter = {31},
  isbn = {978-1-119-95249-7},
  langid = {english},
  keywords = {bedrock channels,bedrock confined,bedrock constrained,channel types,dynamic equilibrium,rock-bed channels}
}

