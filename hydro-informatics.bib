@article{wegscheider2020mesohabitat,
  title = {Mesohabitat Modelling in Fish Ecology: {{A}} Global Synthesis},
  shorttitle = {Mesohabitat Modelling in Fish Ecology},
  author = {Wegscheider, Bernhard and Linnansaari, Tommi and Curry, R. Allen},
