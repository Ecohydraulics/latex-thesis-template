@article{song2011advances,
  title = {Advances in the Study of Uncertainty Quantification of Large-Scale Hydrological Modeling System},
  author = {Song, Xiaomeng and Zhan, Chesheng and Kong, Fanzhe and Xia, Jun},
  year = {2011},
  month = aug,
  journal = {Journal of Geographical Sciences},
  volume = {21},
  number = {5},
  pages = {801},
  issn = {1861-9568},
