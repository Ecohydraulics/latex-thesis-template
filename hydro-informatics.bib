@article{talmon1995laboratory,
  title = {Laboratory Measurements of the Direction of Sediment Transport on Transverse Alluvial-Bed Slopes},
  author = {Talmon, A.M. and Struiksma, N. and Van Mierlo, M.C.L.M.},
  year = {1995},
