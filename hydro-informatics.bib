@incollection{omidvar2010comparative,
  title = {A {{Comparative Study}} of {{CMA-ES}} on {{Large Scale Global Optimisation}}},
  booktitle = {{{AI}} 2010: {{Advances}} in {{Artificial Intelligence}}},
