@article{zhang1997development,
  title = {Development of an Automatic Calibration Scheme for the {{HBV}} Hydrological Model},
  author = {Zhang, Xingnan and Lindstr{\"o}m, G{\"o}ran},
  year = {1997},
  month = oct,
  journal = {Hydrological Processes},
  volume = {11},
  number = {12},
  pages = {1671--1682},
  issn = {08856087, 10991085},
  doi = {10.1002/(SICI)1099-1085(19971015)11:12<1671::AID-HYP497>3.0.CO;2-G},
  urldate = {2020-05-26},
  abstract = {An automatic calibration scheme for the HBV model (ACSH) was developed. The ACSH was based on the physical signi{\textregistered}cance of the model parameters and structure. The inference of hydrologists in the manual calibration was adopted as the guideline. A slight modi{\textregistered}cation of the model structure of the soil routine was suggested to avoid interdependence of the parameters. In total nine parameters, except the snow routine, Fc and MAXBAS, were calibrated automatically in two stages; {\textregistered}rst the soil moisture routine and then the others. There are six sets in two stages in total. Using the Powell method, the parameters in each step were calibrated simultaneously with carefully selected objective functions, and in particular a powerful objective function for the soil moisture routine. The steps were in a {\textregistered}xed order in the ACSH according to the model structure. The optimal values of the model parameters were stable, with the dierent initial values varying in considerable ranges. The automatic calibration gave the same model performance as the manual calibration when the ACSH was tested in two basins. The automatic calibration can thus be used as a reference or as an alternative solution of the model. \# 1997 John Wiley \& Sons, Ltd.},
  langid = {english}
}

