@article{swarnkar2018assessment,
  title = {Assessment of Uncertainties in Soil Erosion and Sediment Yield Estimates at Ungauged Basins: An Application to the {{Garra River}} Basin, {{India}}},
  shorttitle = {Assessment of Uncertainties in Soil Erosion and Sediment Yield Estimates at Ungauged Basins},
  author = {Swarnkar, Somil and Malini, Anshu and Tripathi, Shivam and Sinha, Rajiv},
  year = {2018},
  month = apr,
  journal = {Hydrology and Earth System Sciences},
  volume = {22},
  number = {4},
  pages = {2471--2485},
  issn = {1027-5606},
  doi = {10.5194/hess-22-2471-2018},
  urldate = {2022-02-10},
  abstract = {{$<$}p{$><$}strong class="journal-contentHeaderColor"{$>$}Abstract.{$<$}/strong{$>$} High soil erosion and excessive sediment load are serious problems in several Himalayan river basins. To apply mitigation procedures, precise estimation of soil erosion and sediment yield with associated uncertainties are needed. Here, the revised universal soil loss equation (RUSLE) and the sediment delivery ratio (SDR) equations are used to estimate the spatial pattern of soil erosion (SE) and sediment yield (SY) in the Garra River basin, a small Himalayan tributary of the River Ganga. A methodology is proposed for quantifying and propagating uncertainties in SE, SDR and SY estimates. Expressions for uncertainty propagation are derived by first-order uncertainty analysis, making the method viable even for large river basins. The methodology is applied to investigate the relative importance of different RUSLE factors in estimating the magnitude and uncertainties in SE over two distinct morphoclimatic regimes of the Garra River basin, namely the upper mountainous region and the lower alluvial plains. Our results suggest that average SE in the basin is very high (23\&thinsp;{$\pm$}\&thinsp;4.7\&thinsp;t\&thinsp;ha\textsuperscript{-1}\&thinsp;yr\textsuperscript{-1}) with higher values in the upper mountainous region (92\&thinsp;{$\pm$}\&thinsp;15.2\&thinsp;t\&thinsp;ha\textsuperscript{-1}\&thinsp;yr\textsuperscript{-1}) compared to the lower alluvial plains (19.3\&thinsp;{$\pm$}\&thinsp;4\&thinsp;t\&thinsp;ha\textsuperscript{-1}\&thinsp;yr\textsuperscript{-1}). Furthermore, the topographic steepness (LS) and crop practice (CP) factors exhibit higher uncertainties than other RUSLE factors. The annual average SY is estimated at two locations in the basin {\textendash} Nanak Sagar Dam (NSD) for the period 1962{\textendash}2008 and Husepur gauging station (HGS) for 1987{\textendash}2002. The SY at NSD and HGS are estimated to be 6.9\&thinsp;{$\pm$}\&thinsp;1.2\&thinsp;{\texttimes}\&thinsp;10\textsuperscript{5}\&thinsp;t\&thinsp;yr\textsuperscript{-1} and 6.7\&thinsp;{$\pm$}\&thinsp;1.4\&thinsp;{\texttimes}\&thinsp;10\textsuperscript{6}\&thinsp;t\&thinsp;yr\textsuperscript{-1}, respectively, and the estimated 90\&thinsp;\% interval contains the observed values of 6.4\&thinsp;{\texttimes}\&thinsp;10\textsuperscript{5}\&thinsp;t\&thinsp;yr\textsuperscript{-1} and 7.2\&thinsp;{\texttimes}\&thinsp;10\textsuperscript{6}\&thinsp;t\&thinsp;yr\textsuperscript{-1}, respectively. The study demonstrated the usefulness of the proposed methodology for quantifying uncertainty in SE and SY estimates at ungauged basins.{$<$}/p{$>$}},
  langid = {english}
}

