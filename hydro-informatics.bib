@article{ghobadian2016numerical,
  title = {Numerical Study of Junction-Angle Effects on Flow Pattern in a River Confluence Located in a River Bend},
  author = {Ghobadian, Rasool and Tabar, Zahra Seyd and Koochak, Parisa},
  year = {2016},
  month = jan,
  journal = {Water SA},
  volume = {42},
  number = {1},
  pages = {43},
  issn = {0378-4738},
  doi = {10.4314/wsa.v42i1.06},
  urldate = {2020-05-26},
  abstract = {The effect of main channel curvature on the flow pattern in river junctions is a complex and important issue. The 3-dimensional flow pattern in a river bend with a lateral or tributary channel is not only affected by the centrifugal force and pressure gradient but is also affected by the tributary channel's momentum. Understanding this phenomenon requires extensive research: in this study the effect of 4 tributary junction angles, placed at a 45{\textdegree} angle from the beginning of the bend, is studied using SSIIM1 software. The effect of the junction angle on the vertical and transverse velocity profile, water level changes in the main channel, bed shear-stress distribution and secondary flow strength were evaluated. The results showed that by increasing the junction angle from 30{\textdegree} to 115{\textdegree} the streamwise velocity in the vicinity of the centre line and the inner wall of the bend increases. Increasing the junction angle also increases the separation zone dimensions, maximum bed shear stress, difference between the upstream and downstream water level in the junction and the secondary flow strength.},
  langid = {english}
}

@article{gholami2014experimental,
  title = {Experimental and {{Numerical Study}} on {{Velocity Fields}} and {{Water Surface Profile}} in a {{Strongly-Curved}} 90{\textdegree} {{Open Channel Bend}}},
  author = {Gholami, Azadeh and Akbar Akhtari, Ali and Minatour, Yasser and Bonakdari, Hossein and Javadi, Akbar A.},
  year = {2014},
  month = jan,
  journal = {Engineering Applications of Computational Fluid Mechanics},
  volume = {8},
  number = {3},
  pages = {447--461},
  issn = {1994-2060, 1997-003X},
  doi = {10.1080/19942060.2014.11015528},
  urldate = {2020-05-26},
  abstract = {The flow pattern changes at open channel bend as the result of the presence of centrifugal force. Parameters such as water surface level, the transverse and depth velocity distribution, secondary flow and separation zones deeply depend on bend angel and radius, while many researchers are interested in flow patterns at bends. This paper presents an experimental and numerical study of the flow patterns in a strongly-curved 90{\textordmasculine} open channel bend. The numerical modelling was carried out using the finite volume method. In this numerical study, the k   (RNG) model was used to predict the turbulence, and the volume of fluid (VOF) method was used to simulate the water free surface. The numerical results were verified against data from a program of experiments conducted in this study. A comparison between the experimental data and the results of the numerical model showed that the k   (RNG) and VOF methods are capable of simulating the flow pattern in the strongly-curved bends. The results showed that along a strongly-curved 90{\textordmasculine} bend, the maximum flow velocity always occurs near the inner wall and in such bends the effect of the secondary flows is not limited to the sections within the bend. Also the overall shape of streamlines along the bend and at different water levels is different. In addition, the results revealed that the transverse slope of the water surface is non-linear in these bends. This situation occurs in bends even though maintaining a steady uniform flow condition in straight sections.},
  langid = {english}
}

