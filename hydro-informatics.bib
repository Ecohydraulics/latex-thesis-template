@article{abbasi2004twodimensional,
  title = {Two-Dimensional Simulation of Water Flow and Solute Transport below Furrows: Model Calibration and Validation},
  shorttitle = {Two-Dimensional Simulation of Water Flow and Solute Transport below Furrows},
  author = {Abbasi, Fariborz and Feyen, Jan and {van Genuchten}, M.Th},
  year = {2004},
  month = may,
  journal = {Journal of Hydrology},
  volume = {290},
  number = {1-2},
  pages = {63--79},
  issn = {00221694},
  doi = {10.1016/j.jhydrol.2003.11.028},
  urldate = {2020-05-26},
  abstract = {In this study a two-dimensional numerical flow/transport model (HYDRUS-2D) was calibrated and experimentally validated using data from long furrow irrigation experiments. The model was calibrated using data from an experiment carried out assuming free-draining (FD) outlet conditions, and subsequently validated against data from three experiments assuming blocked-end conditions. The data were analyzed using the Richards' equation for variably saturated flow and either the traditional convection{\textendash} dispersion equation (CDE) or the physical nonequilibrium mobile {\textendash} immobile (MIM) model for solute transport. Optimization was accomplished by means of Levenberg {\textendash} Marquardt optimization in combination with the HYDRUS-2D model. Simultaneous and two-step optimization approaches were used to estimate the soil hydraulic and solute transport parameters near the FD furrow inlet and outlet sites. First, the saturated hydraulic conductivity {\textdh}Ks{\TH} and CDE or MIM solute transport parameters were estimated simultaneously. We also used sequential (two-step) estimation in which we first estimated the soil hydraulic parameters followed by estimation of the solute transport parameters. In the two-step method, the saturated soil water content {\textdh}us{\TH}; the n parameter in van Genuchten's soil water retention model, and Ks values were estimated during the first step, and the CDE or MIM solute transport parameters during the second step.},
  langid = {english}
}

