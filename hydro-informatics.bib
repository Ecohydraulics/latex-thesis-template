@inproceedings{maskey2021sediment,
  title = {Sediment Management Study: {{Case}} Study of {{Kabeli A HEP}}, {{Nepal}}},
  booktitle = {{{IAHR}} 2020 - {{Abstract Book}}},
  author = {Maskey, Diwash Lal and R{\"u}ther, Nils and Stokseth, Siri},
  editor = {Kalinowska, Monika and Rowi{\'n}ski, Pawe{\l} and Okruszko, Tomasz and Nones, Michael},
  year = {2021},
