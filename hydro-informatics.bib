@article{estabrook1998plantplant,
  title = {Plant-{{Plant Communications}}: {{Rhizosphere Signaling}} between {{Parasitic Angiosperms}} and {{Their Hosts1}}},
  shorttitle = {Plant-{{Plant Communications}}},
  author = {Estabrook, Elizabeth M. and Yoder, John I.},
