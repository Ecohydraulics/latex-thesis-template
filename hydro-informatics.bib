@article{afshar2011particle,
  title = {Particle {{Swarm Optimization}} for {{Automatic Calibration}} of {{Large Scale Water Quality Model}} ({{CE-QUAL-W2}}): {{Application}} to {{Karkheh Reservoir}}, {{Iran}}},
