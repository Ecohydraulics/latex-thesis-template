  author = {{Gomez-Gesteira}, M. and Rogers, B. D. and Crespo, A. J. C. and Dalrymple, R. A. and Narayanaswamy, M. and Dominguez, J. M.},
  year = {2012},
  month = nov,
  journal = {Computers \& Geosciences},
  volume = {48},
  pages = {289--299},
  issn = {0098-3004},
  doi = {10.1016/j.cageo.2012.02.029},
  urldate = {2023-12-14},
  abstract = {A free-surface fluid solver called SPHysics is presented. Part 1 provides a description of the governing equations based on Smoothed Particle Hydrodynamics (SPH) theory. The paper describes the formulations implemented in the code including the classical SPH formulation along with enhancements like density filtering, arbitrary Lagrange{\textendash}Euler (ALE) schemes and the incorporation of Riemann solvers for particle{\textendash}particle interactions. Second-order time stepping schemes are presented along with the boundary conditions employed which can handle floating objects to study fluid{\textendash}structure interaction. In addition, the model implementation is briefly described. This information will be used in Part 2, where the efficiency of the code is discussed, along with several study cases.},
  keywords = {Computational fluid dynamics,Free-surface flows,Smoothed particle hydrodynamics}
}

