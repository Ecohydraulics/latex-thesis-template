@article{dariane2016developing,
  title = {Developing an {{Efficient Auto-Calibration Algorithm}} for {{HEC-HMS Program}}},
  author = {Dariane, Alireza B. and Javadianzadeh, M. M. and James, L. Douglas},
  year = {2016},
  month = apr,
  journal = {Water Resources Management},
  volume = {30},
  number = {6},
  pages = {1923--1937},
  issn = {0920-4741, 1573-1650},
  doi = {10.1007/s11269-016-1260-7},
  urldate = {2020-05-26},
  abstract = {Two auto-calibration methods, namely Nelder-Mead (NM) and Univariate-Gradient (UG) and a manual approach are available for calibrating the HEC-HMS program. However, their being either inefficient or time consuming makes it difficult to work with HEC-HMS especially when using a snowmelt module in a continuous mode. The main objective of this paper is to develop an efficient Genetic Algorithm (GA) based auto-calibration method for HEC-HMS model (HMS-GA) for continuous snowmelt simulation. A general novel procedure is presented in the absence of the HMS source code to link a heuristic algorithm with the HEC program through Jython programming language. The models are developed and evaluated using daily data from basins in Ajichai, northwestern Iran. A comparison of results for a verification period indicates a substantial improvement by applying the HMS-GA over the other available methods. Moreover, it is shown that neither NM nor UG is able to improve the results obtained by either the manual or HMS-GA. Furthermore, the proposed method significantly improves the calibrations of the HMS model found by the three other methods.},
  langid = {english}
}

@article{dasilva2006variation,
  title = {Variation of {{Flow Pattern}} with {{Sinuosity}} in {{Sine-Generated Meandering Streams}}},
  author = {{da Silva}, Ana Maria Ferreira and {El-Tahawy}, Tarek and Tape, William D.},
  year = {2006},
  month = oct,
  journal = {Journal of Hydraulic Engineering},
  volume = {132},
  number = {10},
  pages = {1003--1014},
  issn = {0733-9429, 1943-7900},
  doi = {10.1061/(ASCE)0733-9429(2006)132:10(1003)},
  urldate = {2020-05-26},
  abstract = {The effect of channel sinuosity on flow pattern in meandering streams is investigated. The centerlines of the idealized meandering streams under consideration follow sine-generated curves, and the banks are rigid; the flow is turbulent and subcritical. This study focuses on the vertically averaged flow over a flat ͑horizontal at any cross section͒ bed formed by a granular material. The ``flat bed'' is viewed as the initial surface of a moveable bed at the beginning of an experiment ͑at time t = 0͒. A series of laboratory flow measurements involving the systematic variation of the deflection angle ␪0 from 30 to 110{\textdegree}, is used. It is found that every different sinuosity ͑every different ␪0͒ has its own convective flow pattern, i.e., its own distribution in plan of ͑the L / 2 long͒ convergence{\textendash}divergence zones of flow. As ␪0 increases, a gradual change in flow pattern is observed. Two expressions defining the observed ␪0 variation of the convective flow pattern are introduced. It is shown, with the aid of the sediment transport continuity equation, that the geometry of the developed bed at the end of an experiment is strongly related to the convective behavior of the vertically averaged ͑initial͒ flow over the flat bed at t = 0. In particular, information on the ␪0 variation of the convective pattern of the initial flow can be used to estimate the location of erosion{\textendash}deposition zones and the location͑s͒ of the most intense erosion{\textendash}deposition corresponding to any ␪0.},
  langid = {english}
}

