@techreport{vazqueztarrio2022can,
  type = {{{SSRN Scholarly Paper}}},
  title = {Can {{We Wrongly Link Armouring}} to {{Damming}}? {{A Need}} to {{Promote Hypothesis-Driven Rather}} than {{Expert-Based Approaches}} in {{Fluvial Geomorphology}}},
  shorttitle = {Can {{We Wrongly Link Armouring}} to {{Damming}}?},
  author = {V{\'a}zquez Tarr{\'i}o, Daniel and Tal, Michal and Parrot, Elsa and Pi{\'e}gay, Herv{\'e}},
  year = {2022},
  month = feb,
  number = {ID 4027250},
  address = {{Rochester, NY}},
  institution = {{Social Science Research Network}},
  doi = {10.2139/ssrn.4027250},
  urldate = {2022-02-09},
  abstract = {River forms and processes are, in many cases, conditioned by the consequences of anthropogenic modifications, widespread during the last two centuries, such as dams, embankments and gravel-mining. The effects of these human impacts have been typically investigated by fluvial geomorphologists based on a so-called expert-based approach whereby observed association or synchronicity between geomorphological changes and a given, preidentified impact, are interpreted as evidence of causation. This approach has important limitations when the effects of multiple human interventions interact along the same river corridor or overlap with legacy natural changes affecting the sediment - water balance. In such situations, the establishment of causal links between channel morphology and single impacts is not as straightforward as commonly assumed and our conclusions may be affected by `confirmation biases'. In this paper we highlight this risk through an assessment of human impacts on the Rh{\^o}ne river within a multi-driver context. The French Rh{\^o}ne is a perfect example of an Anthropocene river impacted by two main development phases during the 20th century: river training (1890s -1930s) and the construction of a series of dams (1950s -1990s). In this study we began by laying out four hypothetical and geomorphologically consistent scenarios for the geomorphological trajectory followed by the Rh{\^o}ne over the 20th century. Next, we tested these hypothetical scenarios against grain-size data collected in the field in a structured and hypothesis-oriented way. Using this hypothesis-driven and deductive attribution analysis we identified the relative impacts of the different development phases on the present-day grain-size distribution and in particular on armouring in the Rh{\^o}ne river, and proposed a hierarchy of dominant drivers of geomorphological change along the Rhone over the last century and a half. Based on the results of our study, we concluded that in the case of the Rh{\^o}ne, the effect of dams on armouring was negligible compared to a legacy of natural heritages and embankments.},
  langid = {english},
  keywords = {Anthropocene rivers,Dams,Grain-size,Rh{\^o}ne river}
}

