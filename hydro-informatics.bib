@article{vollmer2022incorporating,
  title = {Incorporating {{Ecosystem Services}} into {{Water Resources Management}}{\textemdash}{{Tools}}, {{Policies}}, {{Promising Pathways}}},
  author = {Vollmer, Derek and Burkhard, Kremena and Adem Esmail, Blal and Guerrero, Paulina and Nagabhatla, Nidhi},
