@article{ercan2017scaling,
  title = {Scaling {{Relations}} and {{Self-Similarity}} of 3-{{Dimensional Reynolds-Averaged Navier-Stokes Equations}}},
  author = {Ercan, Ali and Kavvas, M. Levent},
  year = {2017},
