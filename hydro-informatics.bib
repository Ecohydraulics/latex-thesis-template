@article{abeysinghe2019mapping,
  title = {Mapping {{Invasive Phragmites}} Australis in the {{Old Woman Creek Estuary Using UAV Remote Sensing}} and {{Machine Learning Classifiers}}},
  author = {Abeysinghe, Tharindu and Simic Milas, Anita and Arend, Kristin and Hohman, Breann and Reil, Patrick and Gregory, Andrew and {V{\'a}zquez-Ortega}, Ang{\'e}lica},
  year = {2019},
