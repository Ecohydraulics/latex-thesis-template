@incollection{rickenmann2017bedload,
  title = {Bedload {{Transport Measurements}} with {{Geophones}}, {{Hydrophones}}, and {{Underwater Microphones}} ({{Passive Acoustic Methods}})},
  booktitle = {Gravel-{{Bed Rivers}}},
  author = {Rickenmann, Dieter},
  year = {2017},
