@article{mehta1989cohesive,
  title = {Cohesive {{Sediment Transport}}. {{I}}: {{Process Description}}},
  shorttitle = {Cohesive {{Sediment Transport}}. {{I}}},
  author = {Mehta, Ashish J. and Hayter, Earl J. and Parker, W. Reginald and Krone, Ray B. and Teeter, Allen M.},
  year = {1989},
