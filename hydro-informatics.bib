@book{strobl2006wasserbau,
  title = {{Wasserbau: aktuelle Grundlagen - Neue Entwicklungen}},
  shorttitle = {{Wasserbau}},
  author = {Strobl, Theodor and Zunic, Franz},
  year = {2006},
  publisher = {{Springer}},
  address = {{Berlin}},
  abstract = {Die jahrtausende alte Disziplin des Wasserbaus unterlag von jeher einem stetigen Wandel infolge der notwendigen Anpassung an die gesellschaftlichen Bed{\"u}rfnisse und die Anforderungen, die an wasserwirtschaftliche Planung und die konstruktive Ausbildung wasserbaulicher Projekte gestellt wurden. Neue Techniken und Bauformen entstanden insbesondere in den letzten 50 Jahren. In diese Entwicklung flossen die wissenschaftlichen Erkenntnisse aus Forschungsprojekten vieler Universit{\"a}ten. In dem Fachbuch werden die aktuellen Ergebnisse aus Forschungen unter Beachtung des notwendigen Praxisbezuges so aufbereitet, dass diese Ergebnisse auch in die L{\"o}sung der Problemstellungen in der t{\"a}glichen Entwurfsarbeit einflie{\ss}en k{\"o}nnen. Das vorliegende Buch widmet sich in erster Linie an den berufst{\"a}tigen Ingenieur, der in die Planung und Realisierung wasserwirtschaftlicher Projekte eingebunden ist. Damit sind Ingenieurb{\"u}ros ebenso angesprochen wie {\"A}mter und Beh{\"o}rden. Obwohl kein Lehrbuch im strengen Sinne, ist es jedoch f{\"u}r Studierende des Bauingenieurwesens auch von gro{\ss}em Wert. Die umfassende Darstellung aller relevanten Bereiche des Wasserbaus garantieren auch dem Lernenden einen wertvollen Einblick in die aktuellen Aufgaben und L{\"o}sungsans{\"a}tze im modernen Wasserbau. Wasserbau hat heute einen starken Auslandsbezug. Die besonderen Anforderungen an die Planung wasserbaulicher Anlagen im Ausland werden daher ebenso dargestellt. Der Modernisierung und Sanierung bestehender Anlagen widmet sich ein eigenes Kapitel. Das die aktuellen Normen und Regelwerke Eingang in das Buch finden, versteht sich von selbst. Die neue Europ{\"a}ische Wasserrahmenrichtlinie wurde k{\"u}rzlich verabschiedet, und die wichtigste Wasserbau-Norm DIN 19700 wird voraussichtlich in 2004 erscheinen. TOC:Hydrologie.- Flussbau.- Hochwasser.- Wehre.- Talsperren.- Wasserkraft.- Binnenschiffahrt.- Wasserbau im Ausland.- Modernisierung wasserbaulicher Anlagen.- Modelle im Wasserbau.- Planung im Wasserbau.- Zusammenstellung wichtiger Institutionen.- Literatur.- Glossar.-},
  isbn = {978-3-540-22300-9},
  langid = {ngerman},
