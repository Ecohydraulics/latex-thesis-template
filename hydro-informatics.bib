  address = {{Munich, Germany}},
  urldate = {2023-10-18},
  abstract = {A framework, combined the Discrete Element Method (DEM) and Artificial Neural Network (ANN), was developed to predict the porosity and the fine sediment distribution. The porosity variation and fine sediment exchange were integrated to develop bed variation model. The new model was tested on three straight channels. Analyzing the simulated results and comparing with the observed data show that the developed model can successfully reproduce sediment transport, grain sorting processes, and bed change in gravel-bed rivers.},
  school = {Technische Universit{\"a}t M{\"u}nchen}
}

