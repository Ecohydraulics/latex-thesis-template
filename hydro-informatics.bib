@article{acharya2019dedicated,
  title = {Dedicated {{Bioenergy Crops}} and {{Water Erosion}}},
  author = {Acharya, Bharat Sharma and {Blanco-Canqui}, Humberto and Mitchell, Robert B. and Cruse, Richard and Laird, David},
  year = {2019},
  journal = {Journal of Environmental Quality},
  volume = {48},
  number = {2},
  pages = {485--492},
  issn = {1537-2537},
  doi = {10.2134/jeq2018.10.0380},
  urldate = {2022-11-21},
  abstract = {Information on the water quality impact of perennial warm-season grasses (WSGs) when grown in marginal lands as dedicated energy crops is limited. We studied how WSGs affected runoff, sediment, and nutrient losses and related near-surface soil properties to those of no-till corn (Zea mays L.) on an eroded soil in southwestern Iowa and a center pivot corner in east-central Nebraska. The experiment at the eroded soil was established in 2012, and treatments included `Liberty' switchgrass (Panicum virgatum L.) and no-till continuous corn. The experiment at the pivot corner was established in 2013 with `Liberty' switchgrass, `Shawnee' switchgrass, low-diversity grass mixture, and corn. We simulated rainfall at 63.5 {$\pm$} 2.8 mm h-1 for 1 h to portray 5-yr return periods and measured water erosion in spring 2017. Time to runoff start and runoff depth did not differ between WSGs and corn. On the eroded soil, sediment and nutrient losses did not differ between treatments. At the pivot corner, sediment (0.71 vs. 0.15 Mg ha-1) and PO4{\textendash}P (0.037 vs. 0.006 kg ha-1) losses were five times higher in corn than in WSGs. Near-surface soil properties did not differ on the eroded soil, but at the pivot corner, wet aggregate stability was four times higher and residue cover was 34\% higher in WSGs than in corn. Water-stable aggregates were negatively correlated with NO3{\textendash}N and PO4{\textendash}P losses. Overall, WSGs can improve water quality in marginally productive croplands, but their effectiveness appears to be site specific. Core Ideas Warm-season grasses did not reduce runoff depth compared with corn. Warm-season grasses on a center pivot corner reduced sediment and nutrient loss compared with corn. Sediment loss between corn and switchgrass grown in an eroded soil did not differ. Warm-season grass effectiveness to reduce water erosion can be site specific.},
  langid = {english}
}

@book{achleitner20132d,
  title = {{{2D}} Fractional Bed Load Transport in an Alpine Stream: {{Long-}} Term Simulation Coupled with Intermittent Flood Situations},
  author = {Achleitner, Stefan and Klar, Robert and Jocham, S and Aufleger, Markus and Neuner, Johann},
  year = {2013},
  month = oct,
  abstract = {The current work is embedded in a research project, financed by TIWAG {\textendash} Tiroler Wasserkraft AG, aiming at the application and improvement of 2D fractional bed load transport to an alpine river in the {\"O}tztaler Alps in Tyrol/Austria. The catchment with a size of 890 km{$^2$} ranges from 673 and 3762 masl. of which 13\% are glaciated. The roughly 50 km long main river reach is characterized by several steep sections followed by spacious, populated basins. Focus in the work is put on the long term modeling of bed load transport in an alpine stream coupled with intermittent flood situations. The work aims to come up with an increasingly reliable and stable simulation of the river bed development in alpine environment. Thus, this would allow to not only reconstruct historic situations, but as well to test alterations in the river system. This allows testing for e.g. flood protection measures, changed meteorological situations or the combination of both for an enhanced future preparedness. The used two-dimensional software Hydro\_GS-2D is based on the Finite Volume Method to solve the shallow water equations (Nujic, 2009). The implemented bed load transport model uses a multi-fraction multi-layer approach, with a three layer system. The multi-fraction approach is implemented using the Meyer-Peter \& M{\"u}ller (1949) equation coupled with the hiding function by Hunziker (1995) and Hunziker et al. (2009). The investigations are primarily dealing with the long term morphodynamic simulation of periods of several years, respectively the spring/summer periods being relevant for bed load transport. Where the transport of fine material is dominating over long periods, the modeling approach aims to be capable to simulate increased bed load transport during flood events as well. Overall current and altered erosion and deposition dynamics of the bed shall be simulated. Although significant advances in the field were already made during the last years, the computational demands are a bottle neck of such elaborate modeling approaches. This is even more relevant when larger areas are to be simulated over long periods as it is the case here. A distributed computing approach to reduce simulation times is presented, enabling the spatial and temporal decomposition of simulations. The gained speedup is estimated using simulations of separated test cases. For the calibration of the actual model the initial states at the simulation start are a core issue. The given catchment is exceptionally well covered by field investigations regarding grain size distribution (line-by-number analysis and sieve analysis) and cross section profiles along the main river. Still, the spatially distributed initial grain size distribution and associated skin friction coefficients are altered with time. The temporal variation of the bed roughness is equally important as the initial parameterization. For sake of computational demands an external approach enabling the modification of skin friction and/or roughness coefficient is realized on a section wise basis. As the approach is mostly driven by the actual grain size distribution, the approach is tested in a sub basin under flood flow conditions, bearing the most significant morphological changes. Still, although the required model inputs are best possible estimated, the model results bear a remaining uncertainty.}
}

