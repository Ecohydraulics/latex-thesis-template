@article{vandenderen2019explaining,
  title = {Explaining Artificial Side Channel Dynamics Using Data Analysis and Model Calculations},
  author = {{van Denderen}, Pepijn R. and Schielen, Ralph M.J. and Westerhof, Sam G. and Quartel, Susanne and Hulscher, Suzanne J.M.H.},
