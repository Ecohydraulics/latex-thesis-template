@article{marini2015particle,
  title = {Particle Swarm Optimization ({{PSO}}). {{A}} Tutorial},
  author = {Marini, Federico and Walczak, Beata},
  year = {2015},
  month = dec,
  journal = {Chemometrics and Intelligent Laboratory Systems},
  volume = {149},
  pages = {153--165},
