@article{harper2017complexities,
  title = {The Complexities of Measuring Fine Sediment Accumulation within Gravel-Bed Rivers},
  author = {Harper, S.E. and Foster, I.D.L. and Lawler, D.M. and Mathers, K.l. and McKenzie, M. and Petts, G.E.},
  year = {2017},
  journal = {River Research and Applications},
  volume = {33},
  number = {10},
  pages = {1575--1584},
  issn = {1535-1467},
  doi = {10.1002/rra.3198},
  urldate = {2023-08-18},
  abstract = {Fine sediment storage within gravel beds is a key component of catchment sediment budgets and affects the health of benthic and hyporheic habitats. Here, we assess the performance of two substrate infiltration traps for the characterization of fine sediment ({$<$}2 mm) accumulation. One design, the vertically extending sediment trap, permits both lateral and vertical exchange in the sediment column, whereas the second type, a more traditional fixed-area sediment trap with impermeable side walls, permits only vertical exchange. Traps were deployed at three sites on the River Tame, Birmingham (UK), over varying installation periods (14{\textendash}401 days). Results indicate that the facilitation of multiple pathways of exchange within the vertically extending sediment traps (vertical and lateral) resulted in a significantly greater amount of fine sediment being accumulated than in adjacent fixed-area sediment traps. This suggests that lateral transport is an important component contributing to fine sediment accumulation. However, there are notable and inherent problems associated with the use of different types of sediment trap and in the way the data should be presented and interpreted. This paper discusses the practical implications of the study findings and reflects on the complexities of undertaking accurate sediment deposition measurements in the field.},
  copyright = {{\textcopyright} 2017 The Authors River Research and Applications Published by John Wiley \& Sons Ltd},
  langid = {english},
  keywords = {fine sediment,gravel bed rivers,hydrological exchange,sediment trap design,sedimentation,vertical and lateral transport}
}

