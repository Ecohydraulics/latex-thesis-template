@article{oger2007improved,
  title = {An Improved {{SPH}} Method: {{Towards}} Higher Order Convergence},
  shorttitle = {An Improved {{SPH}} Method},
  author = {Oger, G. and Doring, M. and Alessandrini, B. and Ferrant, P.},
