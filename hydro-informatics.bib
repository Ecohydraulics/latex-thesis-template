@article{youschkevitch1983kolmogorov,
  title = {A. {{N}}. {{Kolmogorov}}: {{Historian}} and Philosopher of Mathematics on the Occasion of His 80th Birthday},
  shorttitle = {A. {{N}}. {{Kolmogorov}}},
  author = {Youschkevitch, A. P},
  year = {1983},
  month = nov,
  journal = {Historia Mathematica},
  volume = {10},
  number = {4},
  pages = {383--395},
  issn = {0315-0860},
  doi = {10.1016/0315-0860(83)90001-0},
  urldate = {2023-03-31},
  abstract = {The contributions to the philosophy and history of mathematics of one of the most eminent contemporary mathematicians, A. N. Kolmogorov, are analyzed. Although these contributions are not numerous, they contain discussions of the gradual generalization of the subject of mathematics, a partition of the development of mathematics{\textemdash}in particular, of the theory of probability{\textemdash}into periods, and, also, an evaluation of the works of a number of great savants, such as Newton, Lobatchevsky, etc. R{\'e}sum{\'e} L'article analyse l'apport de l'un des plus {\'e}minents math{\'e}maticiens de notre temps, Andrei Kolmogorov, {\`a} la philosophie et {\`a} l'histoire des math{\'e}matiques. Ses articles, pas tr{\`e}s nombreux, traitent de la g{\'e}n{\'e}ralisation graduelle de l'objet des math{\'e}matiques et d'une certaine p{\'e}riodisation du cheminement des math{\'e}matiques (en particulier de la th{\'e}orie des probabilit{\'e}). Ils {\'e}valuent aussi l'activit{\'e} de nombreux savants tels que Newton, Lobatchevsky, etc. Zusammenfassung Der Artikel enth{\"a}lt eine knappe Analyse des Beitrages Andrei Kolmogorovs, eines prominenten Mathematikers der Gegenwart, zur Philosophie und Geschichte der Mathematiik. Nicht zahl-, aber inhaltsreich, schildern die entsprechenden Werke Kolmogorovs die allm{\"a}hliche Verallgemeinerung der Mathematik, geben eine originelle Periodisierung ihrer Entwicklung im grossen und ganzen und insbesondere der Wahrscheinlichkeitsrechnung sowie seine Einsch{\"a}tzung der Leistungen verschiedener grosser Gelehrter, wie Newton, Lobatchevsky, usw.},
  langid = {english}
}

