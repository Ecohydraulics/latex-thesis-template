@article{ruther2005threedimensional,
  title = {Three-{{Dimensional Modeling}} of {{Sediment Transport}} in a {{Narrow}} 90{\textdegree} {{Channel Bend}}},
  author = {R{\"u}ther, Nils and Olsen, Nils Reidar},
  year = {2005},
  month = oct,
  journal = {Journal of Hydraulic Engineering},
  volume = {131},
  number = {10},
  pages = {917--920},
  issn = {0733-9429, 1943-7900},
  doi = {10.1061/(ASCE)0733-9429(2005)131:10(917)},
  urldate = {2020-05-26},
  abstract = {A three-dimensional numerical model was used for calculating the velocity and bed level changes over time in a 90{\textdegree} bended channel. The numerical model solved the Reynolds-averaged Navier-Stokes equations in three dimensions to compute the water flow and used the finite-volume method as the discretization scheme. The k-␧ model predicted the turbulence, and the SIMPLE method computed the pressure. The suspended sediment transport was calculated by solving the convection diffusion equation and the bed load transport quantity was determined with an empirical formula. The model was enhanced with relations for the movement of sediment particles on steep side slopes in river bends. Located on a transversally sloping bed, a sediment particle has a lower critical shear stress than on a flat bed. Also, the direction of its movement deviates from the direction of the shear stress near the bed. These phenomenona are considered to play an important role in the morphodynamic process in sharp channel bends. The calculated velocities as well as the bed changes over time were compared with data from a physical model study and good agreement was found.},
  langid = {english}
}

