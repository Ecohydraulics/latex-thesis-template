@article{bais2001rootspecific,
  title = {Root-Specific Metabolism: {{The}} Biology and Biochemistry of Underground Organs},
  shorttitle = {Root-Specific Metabolism},
  author = {Bais, Harsh Pal and {Loyola-Vargas}, Victor M. and Flores, Hector E. and Vivanco, Jorge M.},
