@article{mbonimpa2015combined,
  title = {Combined {{PEST}} and {{Trial}}{\textendash}{{Error}} Approach to Improve {{APEX}} Calibration},
  author = {Mbonimpa, E.G. and Gautam, S. and Lai, L. and Kumar, S. and Bonta, J.V. and Wang, X. and Rafique, R.},
  year = {2015},
