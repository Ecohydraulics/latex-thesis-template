@article{kimiaghalam2016experimental,
  title = {An Experimental Study on the Effects of Physical, Mechanical, and Electrochemical Properties of Natural Cohesive Soils on Critical Shear Stress and Erosion Rate},
  author = {Kimiaghalam, Navid and Clark, Shawn P. and Ahmari, Habib},
  year = {2016},
  month = mar,
  journal = {International Journal of Sediment Research},
  volume = {31},
  number = {1},
  pages = {1--15},
  issn = {10016279},
  doi = {10.1016/j.ijsrc.2015.01.001},
  urldate = {2019-08-06},
  abstract = {The field of cohesive sediment erosion is still not fully understood, in large part due to the many soil parameters that affect cohesive sediment erodibility. In this study several undisturbed natural soil samples were taken from different river banks in Manitoba, Canada. The samples mainly contained clay and silt with 24{\textendash}94\% clay content, thus the study covered a wide range of cohesive soil. For each sample 13 different physical, mechanical, and electrochemical properties were measured. Critical shear stress of erosion and erosion rate were quantified using an Erosion Measurement Device (EMD). Stepwise regression was used to find the variables most significantly correlated to critical shear stress and erosion rate, which led to the development of a new empirical equation to estimate the critical shear stress of cohesive soils. It was found that the critical shear stress is highly correlated with cohesion, while both cohesion and sodium adsorption ratio (SAR) had significant influence on the erosion rate.},
  langid = {english}
}

