@article{uber2022climate,
  title = {Climate {{Change Impacts}} on {{Soil Erosion}} and {{Sediment Delivery}} to {{German Federal Waterways}}: {{A Case Study}} of the {{Elbe Basin}}},
  shorttitle = {Climate {{Change Impacts}} on {{Soil Erosion}} and {{Sediment Delivery}} to {{German Federal Waterways}}},
  author = {Uber, Magdalena and R{\"o}ssler, Ole and Astor, Birgit and Hoffmann, Thomas and Van Oost, Kristof and Hillebrand, Gudrun},
  year = {2022},
  journal = {Atmosphere},
  volume = {13},
