@article{yu2020one,
  title = {One {{Algorithm May Not Fit All}}: {{How Selection Bias Affects Machine}}                    {{Learning Performance}}},
  shorttitle = {One {{Algorithm May Not Fit All}}},
  author = {Yu, Alice C. and Eng, John},
  year = {2020},
  month = nov,
  journal = {RadioGraphics},
  volume = {40},
  number = {7},
  pages = {1932--1937},
  publisher = {{Radiological Society of North America}},
  issn = {0271-5333},
  doi = {10.1148/rg.2020200040},
  urldate = {2023-11-29},
  abstract = {Machine learning (ML) algorithms have demonstrated high diagnostic accuracy in identifying and categorizing disease on radiologic images. Despite the results of initial research studies that report ML algorithm diagnostic accuracy similar to or exceeding that of radiologists, the results are less impressive when the algorithms are installed at new hospitals and are presented with new images. This phenomenon is potentially the result of selection bias in the data that were used to develop the ML algorithm. Selection bias has long been described by clinical epidemiologists as a key consideration when designing a clinical research study, but this concept has largely been unaddressed in the medical imaging ML literature. The authors discuss the importance of selection bias and its relevance to ML algorithm development to prepare the radiologist to critically evaluate ML literature for potential selection bias and understand how it might affect the applicability of ML algorithms in real clinical environments. {\textcopyright}RSNA, 2020}
}

@article{yu2021measures,
  title = {Measures of Uncertainty for a Fuzzy Probabilistic Information System},
  author = {Yu, Guangji},
  year = {2021},
  month = jul,
  journal = {International Journal of General Systems},
  volume = {50},
  number = {5},
  pages = {580--618},
  publisher = {{Taylor \& Francis}},
  issn = {0308-1079},
  doi = {10.1080/03081079.2021.1942866},
  urldate = {2022-12-02},
  abstract = {Fuzzy probabilistic information system (FPIS), a combination of some fuzzy relations on the same universe which satisfies probability distribution, can be regarded as an information system (IS) with fuzzy relations under the probability environment. This means that three types of uncertainty: roughness fuzziness and probability are integrated into an IS. This paper aims to measure the uncertainty of a FPIS. Firstly, information granulation in a FPIS is proposed. Then, information entropy and rough entropy in a FPIS are proposed. Moreover, information amount in a FPIS is also investigated. Finally, the feasibility of the proposed measures are verified by a numerical example, and the statistical analysis is carried out. Those obtained results may be useful for understanding the essence of uncertainty for a FPIS.},
  keywords = {entropy,FPIS,Fuzzy relation,information granulation,measure,uncertainty}
}

@article{yuan2009swarm,
  title = {Swarm Intelligence Optimization and Its Application in Geophysical Data Inversion},
  author = {Yuan, Sanyi and Wang, Shangxu and Tian, Nan},
  year = {2009},
