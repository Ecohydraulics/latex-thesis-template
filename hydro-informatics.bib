  journal = {Proceedings of the Royal Society of London. Series A: Mathematical and Physical Sciences},
  volume = {451},
  number = {1941},
  pages = {141--164},
  publisher = {{Royal Society}},
  doi = {10.1098/rspa.1995.0121},
  urldate = {2023-07-05},
  abstract = {The need for suitable methods to simulate the effects of turbulence in hydraulics is highlighted by discussing the importance of turbulent exchange processes in a number of typical hydraulic flow situations. The possible calculation approaches are reviewed with the conclusion that, for practical calculations, the Reynolds-average-modelling approach is presently the most advanced and most suitable. The turbulence models mainly in use for hydraulic calculations are discussed, with the focus on extensions to account for free-surface effects and on special depth-average versions. A number of calculation examples are presented which demonstrate the possibilities opened up by Reynolds-average-modelling in hydraulics and the impact this modelling has had in this field.}
}

