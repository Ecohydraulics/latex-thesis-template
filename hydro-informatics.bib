@article{pitondebris,
  title = {Debris Flows, Boulders and Constrictions: A Simple Framework for Modeling Jamming, and Its Consequences on Outflow},
  shorttitle = {Debris Flows, Boulders and Constrictions},
  author = {Piton, Guillaume and Goodwin, G. R. and Mark, Emily and Strouth, Alex},
  journal = {Journal of Geophysical Research: Earth Surface},
  volume = {n/a},
  number = {n/a},
  pages = {e2021JF006447},
  issn = {2169-9011},
  doi = {10.1029/2021JF006447},
  urldate = {2022-05-06},
  abstract = {Large boulders can jam in constrictions, both in canyons) and man-made structures (e.g. slit-dams). Boulder jamming occurs stochastically, and partially governs the outflow rate of debris flow material. Explicit hydro-mechanical models of boulder-laden flows are too computationally demanding to study this stochasticity. This study presents a new framework implemented into a numerical program. This program encapsulates basic hydraulic equations and criteria predicting boulder blockage at narrow sections, along with a simple statistical way to compute boulder sizes and numbers. The program applies empirical estimates by experts of the average number of boulders in a deposit to evaluate how equivalent multi-phase flows would interact with a constriction. The model stochastically generate boulders that can obstruct the constriction(s). The program outputs simple descriptions of the upstream flow level over time, as well as the downstream outlet discharge rate and volume. The model is fast (5{\textendash}30 s per run), allowing uncertainty propagation analyses of interactions between flows, boulders and constrictions. Interaction between both low- and high-risk flows can be quickly evaluated for different scenarios. For uncertainty propagation, we use possibility theory, which accommodates uncertainties relevant to debris flows. The framework gives practitioners a much-needed generalized approach for understanding the long-term behavior of boulder-laden flows through canyons, or for designing engineered structures. Finally, we apply the model to a field case {\textendash} the design of a proposed North American slit-dam {\textendash} thence elucidating the design requirements for effective flow control. We found that horizontal bars are necessary for dependably controlling outflow from this structure.},
  langid = {english}
}

