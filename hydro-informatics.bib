@article{wu20003d,
  title = {{{3D Numerical Modeling}} of {{Flow}} and {{Sediment Transport}} in {{Open Channels}}},
  author = {Wu, Weiming and Rodi, Wolfgang and Wenka, Thomas},
  year = {2000},
  month = jan,
  journal = {Journal of Hydraulic Engineering},
  volume = {126},
  number = {1},
  pages = {4--15},
  issn = {0733-9429, 1943-7900},
  doi = {10.1061/(ASCE)0733-9429(2000)126:1(4)},
  urldate = {2020-05-26},
  abstract = {A 3D numerical model for calculating flow and sediment transport in open channels is presented. The flow is calculated by solving the full Reynolds-averaged Navier-Stokes equations with the k Ϫ {$\epsilon$} turbulence model. Special free-surface and roughness treatments are introduced for open-channel flow; in particular the water level is determined from a 2D Poisson equation derived from 2D depth-averaged momentum equations. Suspended-load transport is simulated through the general convection-diffusion equation with an empirical settling-velocity term. This equation and the flow equations are solved numerically with a finite-volume method on an adaptive, nonstaggered grid. Bed-load transport is simulated with a nonequilibrium method and the bed deformation is obtained from an overall mass-balance equation. The suspended-load model is tested for channel flow situations with net entrainment from a loose bed and with net deposition, and the full 3D total-load model is validated by calculating the flow and sediment transport in a 180{\cyrchar\CYRNJE} channel bend with movable bed. In all cases, the agreement with measurements is generally good.},
  langid = {english}
}

@article{wu2000nonuniform,
  title = {Nonuniform Sediment Transport in Alluvial Rivers},
  author = {Wu, Weiming and Wang, Sam S.Y. and Jia, Yafei},
  year = {2000},
  month = nov,
  journal = {Journal of Hydraulic Research},
  volume = {38},
  number = {6},
  pages = {427--434},
  publisher = {{Taylor \& Francis}},
  issn = {0022-1686},
  doi = {10.1080/00221680009498296},
  urldate = {2021-09-21},
  abstract = {A correction factor has been developed in this paper to account for the hiding and exposure mechanism of nonuniform sediment transport. This factor is assumed to be a function of the hidden and exposed probabilities, which are stochastically related to the size and gradation of bed materials. Based on this concept, the formulas to calculate the critical shear stress of incipient motion and the fractional bed-load and suspended load transport rates of nonuniform sediment have been established. These formulas have been tested against a wide range of laboratory and field data and compared with several other existing empirical methods. The predictions by these newly proposed formulas are very good.}
}

