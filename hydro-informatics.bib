@incollection{zhen-gang2017hydrodynamics,
  title = {Hydrodynamics},
  booktitle = {Hydrodynamics and {{Water Quality}}: {{Modeling Rivers}}, {{Lakes}}, and {{Estuaries}}},
  author = {{Zhen-Gang}, J.},
  year = {2017},
  edition = {2nd},
