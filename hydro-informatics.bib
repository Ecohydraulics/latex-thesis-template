  title = {Aquatic Food Security: Insights into Challenges and Solutions from an Analysis of Interactions between Fisheries, Aquaculture, Food Safety, Human Health, Fish and Human Welfare, Economy and Environment},
  shorttitle = {Aquatic Food Security},
  author = {Jennings, Simon and Stentiford, Grant D. and Leocadio, Ana M. and Jeffery, Keith R. and Metcalfe, Julian D. and Katsiadaki, Ioanna and Auchterlonie, Neil A. and Mangi, Stephen C. and Pinnegar, John K. and Ellis, Tim and Peeler, Edmund J. and Luisetti, Tiziana and Baker-Austin, Craig and Brown, Mary and Catchpole, Thomas L. and Clyne, Fiona J. and Dye, Stephen R. and Edmonds, Nathan J. and Hyder, Kieran and Lee, Janette and Lees, David N. and Morgan, Owen C. and O'Brien, Carl M. and Oidtmann, Birgit and Posen, Paulette E. and Santos, Ana Ribeiro and Taylor, Nick G. H. and Turner, Andrew D. and Townhill, Bryony L. and Verner-Jeffreys, David W.},
