@article{mccullough1979bioenergetics,
  title = {Bioenergetics of a Stream ``Collector'' Organism, {{Tricorythodes}} Minutus ({{Insecta}}: {{Ephemeroptera}})1,2},
  shorttitle = {Bioenergetics of a Stream ``Collector'' Organism, {{Tricorythodes}} Minutus ({{Insecta}}},
  author = {McCullough, Dale A. and Minshall, G. Wayne and Cushing, Colbert E.},
  year = {1979},
  journal = {Limnology and Oceanography},
  volume = {24},
  number = {1},
  pages = {45--58},
  issn = {1939-5590},
  doi = {10.4319/lo.1979.24.1.0045},
  urldate = {2023-09-18},
  abstract = {The energy budget of a fine-particle collector organism, Tricorythodes minutus Traver (Ephemeroptera: Tricorythidae), was determined under conditions (flow, substratum, temperature) representative of its natural habitat. Digestion time (gut clearance) was rapid (30 min). Ingestion values ranged widely: 33{\textendash}220 {\textmu}g dry wt{$\cdot$}mg dry wt-1{$\cdot$}h-1(= wW{$\cdot$}h-1), depending on the type of food provided. Assimilation rates were lowest {$\sim$}2 {\textmu}g ash-free dry wt{$\cdot$}mg dry wt-1{$\cdot$}h-1 (= aW{$\cdot$}h-1), with mixed diatoms, intermediate with the blue-green alga Anabaena ({$\sim$}23), and high with the blue-green Lyngbya ({$\sim$}61) and with a pure culture of Nitzschia ({$\sim$}62). Assimilation efficiencies showed a much narrower spread, being 33, 34, and 57\% for mixed diatoms, blue-green algae, and Nitzschia. A mean respiration rate of 2.51 {\textmu}l O2{$\cdot$}mg dry wt-1{$\cdot$}h-1 was derived for T. minutus at 20{\textdegree}C. Growth, estimated from length-weight relationships, was 4.5 wW{$\cdot$}h-1. The instantaneous growth rate for T. minutus (0.126) is higher than those reported for any other mayfly (0.034{\textendash}0.060) except Tricorythodes atratus (0.153). Cast skins accounted for over 30\% of the total production (G + Ex = 6.5 wW{$\cdot$}h-1). Comparison with Stenonema pulchellum, the only other mayfly for which complete energy budget information is available, showed respiration rates to be similar and assimilation and growth of Tricorythodes to be 2.9 and 5.3 times higher. However, the major distinction between the two was the high ingestion rate for Tricorythodes compared with that for Stenonema (7 wW{$\cdot$}h-1).},
  copyright = {{\textcopyright} 1979, by the Association for the Sciences of Limnology and Oceanography, Inc.},
  langid = {english}
}

